// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PSQL_DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  username    String
  password String
  //   posts   Post[]
  //   profile Profile?
}